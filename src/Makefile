all: projet
projet: Plateau.o Case.o main.cpp
	g++ Plateau.o Case.o main.cpp -o projet	
Plateau.o: Plateau.cpp
	g++ -c Plateau.cpp
Case.o: Case.cpp
	g++ -c Case.cpp


